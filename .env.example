# Google Apps Script URL for products data
VITE_APPS_SCRIPT_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Google Apps Script URL for orders submission
VITE_ORDERS_SCRIPT_URL=https://script.google.com/macros/s/YOUR_ORDERS_SCRIPT_ID/exec

# WhatsApp Business Number (without + or spaces, e.g., 919876543210)
VITE_WHATSAPP_NUMBER=919876543210

# Shop Information
VITE_SHOP_NAME=Much Shop

# Checkout Configuration
VITE_DELIVERY_FEE=30
VITE_PREP_TIME_MINUTES=30
VITE_SHOP_OPEN_HOUR=9
VITE_SHOP_CLOSE_HOUR=21

# Firebase Configuration (optional - defaults are set in firebase.js)
# VITE_FIREBASE_API_KEY=your_api_key_here
# VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
# VITE_FIREBASE_PROJECT_ID=your_project_id
# VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
# VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
# VITE_FIREBASE_APP_ID=your_app_id
# VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id
